public class ThreadEx15 {
	public static void main(String[] args) {
		
	}
}

class RunIpleEx15 implements Runnable{
	public void run(){
		while(true){
			System.out.println(Thread.currentThread().getName());
			try {
				Thread.sleep(1000);
			} catch (Exception e) {
				// TODO: handle exception
			}
		}
	}
}
